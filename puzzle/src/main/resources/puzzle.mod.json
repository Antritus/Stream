{
  "formatVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_developer}"
  ],
  "meta": {
    "icon": "${mod_id}:icons/icon.png"
  },
  "entrypoints": {
    "client_init": [],
    "init": [
      "${puzzle_source_initializer}"
    ],
    "preInit": [
      "${puzzle_source_pre_initializer}"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "cosmic-reach": ">=${cosmic_reach_version}",
    "puzzle-loader": ">=${puzzle_loader_version}"
  },
  "accessManipulator": "${mod_id}.manipulator"
}
